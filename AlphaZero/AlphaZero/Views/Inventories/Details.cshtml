@model AlphaZero.Models.inventory

@{
    ViewBag.Title = "Details";
}

<h2>Details of Item</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Inventory Name
        </dt>

        <dd>
            @Html.DisplayFor(model => model.inventory_name)
        </dd>

        <dt>
            Inventory Count
        </dt>

        <dd>
            @Html.DisplayFor(model => model.inventory_count)
        </dd>

        <dt>
            FLoor Level
        </dt>

        <dd>
            @Html.DisplayFor(model => model.floor.floor_id)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.Inventory_id }, new { @class = "btn btn-warning" }) |
    <button onclick="location.href='@Url.Action("Index")'" class="btn ">Back to List</button>
</p>
